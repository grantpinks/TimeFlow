{
  "expo": {
    "name": "TimeFlow",
    "slug": "timeflow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/branding/flow-default.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/branding/main_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8FAFC"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.timeflow.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/branding/flow-default.png",
        "backgroundColor": "#0BAF9A"
      },
      "package": "com.timeflow.app"
    },
    "web": {
      "favicon": "./assets/branding/flow-default.png"
    },
    "scheme": "timeflow"
  }
}
