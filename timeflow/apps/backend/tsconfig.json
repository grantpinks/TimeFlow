{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": ".",
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "paths": {
      "@timeflow/shared": ["../../packages/shared/src"],
      "@timeflow/shared/*": ["../../packages/shared/src/*"],
      "@timeflow/scheduling": ["../../packages/scheduling/src"],
      "@timeflow/scheduling/*": ["../../packages/scheduling/src/*"]
    }
  },
  "include": ["src"]
}


